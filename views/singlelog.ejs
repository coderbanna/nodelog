<!-- logs.ejs -->
<%- include('./partials/header'); %>

<%- include('./partials/navbar'); %>

<!-- end of navbar navigation -->
<div class="content">
    <div class="container">
        <div class="row">
            <div class="col-md-12 page-header">
                <div class="page-pretitle">Log Detail</div>
            </div>
        </div>
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <h5 class="card-header"><%= data.title %></h5>
                        <div class="card-body">
                            <code>
                                <pre>
                                    <%= data.logmsg %>
                                </pre>
                            </code>
                        </div>
                        <div class="card-footer text-end">
                            <button class="btn btn-primary">Submit</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
      
    </div>
</div>

<%- include('./partials/footer'); %>